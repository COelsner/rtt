<xs:schema xmlns:rtt="regression.test.tool" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	attributeFormDefault="unqualified" elementFormDefault="qualified"
	targetNamespace="regression.test.tool">
	
	<xs:simpleType name="type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FIELD" />
			<xs:enumeration value="METHOD" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="value">
		<xs:sequence>
			<xs:element name="node" type="rtt:node" 
				minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="informational" type="xs:boolean" default="false"/>
		<xs:attribute name="isNull" type="xs:boolean" default="false"/>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="type" type="rtt:type" use="required" />
		<xs:attribute name="value" type="xs:string" use="required" />
	</xs:complexType>

	<xs:complexType name="node">
		<xs:sequence>
			<xs:element name="values" type="rtt:value"
				minOccurs="0" maxOccurs="unbounded" />		
			</xs:sequence>
			<xs:attribute name="simpleName" type="xs:string" />
			<xs:attribute name="fullName" type="xs:string" />
	</xs:complexType>
		
	<xs:element name="output" >
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AST" type="rtt:value" 
					minOccurs="1" maxOccurs="1"/>
			</xs:sequence>
			<xs:attribute name="executor" type="xs:string" />
		</xs:complexType>
	</xs:element>
</xs:schema>


