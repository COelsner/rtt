<xs:schema xmlns:rtt="regression.test.tool" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	attributeFormDefault="unqualified" elementFormDefault="qualified"
	targetNamespace="regression.test.tool">
	
	<xs:simpleType name="generatorType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FIELD" />
			<xs:enumeration value="METHOD" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="node">
		<xs:attribute name="informational" type="xs:boolean" default="false"/>
		<xs:attribute name="isNull" type="xs:boolean" default="false"/>
		<xs:attribute name="generatorName" type="xs:string" use="required"/>
		<xs:attribute name="generatorType" type="rtt:generatorType" use="required"/>		
	</xs:complexType>
	
	<xs:complexType name="valueNode">
		<xs:complexContent>
			<xs:extension base="rtt:node">
				<xs:attribute name="value" type="xs:string" use="required" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="classNode">
		<xs:complexContent>
			<xs:extension base="rtt:node">
				<xs:sequence>
					<xs:element name="simpleName" type="xs:string" />
					<xs:element name="fullName" type="xs:string" />
					<xs:element name="node" type="rtt:node" 
						minOccurs="1" maxOccurs="unbounded" />
					</xs:sequence>
			</xs:extension>		
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="output">
		<xs:sequence>
			<xs:element name="node" type="rtt:node" 
				minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:element name="output" type="rtt:output" />
</xs:schema>


