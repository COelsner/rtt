      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="rtt.ui.launchConfigurationType"
            icon="icons/generation.gif"
            id="rtt.ui.launchConfigurationTypeImage">
      </launchConfigurationTypeImage>
