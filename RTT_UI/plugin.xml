      <handler
            commandId="rtt.ui.commands.project.convert">
         <class
               class="rtt.ui.handlers.archive.ConvertProjectHandler"></class>
      </handler>
      <handler
            commandId="rtt.ui.commands.refresh">
         <class
               class="rtt.ui.handlers.archive.RefreshHandler">
         </class>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="rtt.ui.perspectives.ProjectPerspectiveFactory"
            icon="icons/RTT-Logo.png"
            id="rtt.ui.perspective.projectPerspective"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="rtt.ui.perspective.projectPerspective">
         <actionSet
               id="org.eclipse.debug.ui.launchActionSet">
         </actionSet>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="rtt.ui.definitions.selection.archive">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.main.ArchiveContent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.project">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.main.ProjectContent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.directory.testsuite">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.main.TestsuiteDirectory">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.directory.config">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.configuration.ConfigurationDirectory">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.testsuite">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.testsuite.TestsuiteContent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.testcase">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.testsuite.TestcaseContent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.config">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.configuration.ConfigurationContent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.testresult">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.logging.TestResultContent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.selection.comment">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="rtt.ui.content.logging.CommentContent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.configParent">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <test
                     args="configuration"
                     property="rtt.ui.content.hasParent"
                     value="configuration">
               </test>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="rtt.ui.definitions.caseParent">
         <with
               variable="activeMenuSelection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <test
                     args="testcase"
                     property="rtt.ui.content.hasParent"
                     value="testcase">
               </test>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="rtt.ui.editors.ReferenceEditor"
            default="false"
            icon="icons/RTT-Logo.png"
            id="rtt.ui.editors.reference"
            name="%rtt.ui.editor.name">
