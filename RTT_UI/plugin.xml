            extensions="ecore"
            icon="icons/sample.gif"
            id="rtt.ui.editors.annotation.ecore"
            name="%rtt.ecore.editor.name">
         <contentTypeBinding
               contentTypeId="org.eclipse.emf.ecore">
         </contentTypeBinding>
      </editor>
   </extension>
