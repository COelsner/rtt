      </command>
      <command
            id="rtt.ui.commands.tests.generate"
            name="%commands.tests.generate.name">
      </command>
      <command
            id="rtt.ui.commands.tests.run"
            name="%commands.tests.run.name">
      </command>
      <command
            id="rtt.ui.commands.testsuite.add"
            name="%commands.testsuite.add.name">
      </command>
      <command
            id="rtt.ui.commands.testsuite.remove"
            name="%commands.testsuite.remove.name">
      </command>
      <command
            id="rtt.ui.commands.tests.junitexport"
            name="%commands.tests.junitexport.name">
      </command>
      <command
            id="rtt.ui.commands.testcase.add"
            name="%commands.testcase.add.name">
      </command>
      <command
            id="rtt.ui.commands.testcase.remove"
            name="%commands.testcase.remove.name">
      </command>
      <command
            id="rtt.ui.commands.testcase.parameters"
            name="%commands.testcase.parameters.name">
      </command>
      <command
            id="rtt.ui.commands.comment.add"
            name="%commands.comment.add.name">
      </command>
      <command
            id="rtt.ui.commands.comment.edit"
            name="%commands.comment.edit.name">
      </command>
      <command
            id="rtt.ui.commands.comment.remove"
            name="%commands.comment.remove.name">
      </command>
      <command
            id="rtt.ui.commands.refresh"
            name="%rtt.ui.commands.refresh.name">
      </command>
      <command
            id="rtt.ui.commands.ecore.generateAnnotations"
            name="%rtt.ui.ecore.command.generate">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <menu
               label="%menu.label">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                     </adapt>
                  </iterate>
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
            <command
                  commandId="rtt.ui.commands.project.convert"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeMenuSelection">
                     <iterate
                           ifEmpty="false"
                           operator="or">
                        <adapt
                              type="org.eclipse.core.resources.IProject">
                           <not>
                              <test
                                    property="org.eclipse.core.resources.projectNature"
                                    value="rtt.ui.projectnature">
                              </test>
                           </not>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
         <menu
               label="%menu.label">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                     </adapt>
                  </iterate>
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
            <command
                  commandId="rtt.ui.commands.project.convert"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeMenuSelection">
                     <iterate
                           ifEmpty="false"
                           operator="or">
                        <adapt
                              type="org.eclipse.core.resources.IProject">
                           <not>
                              <test
                                    property="org.eclipse.core.resources.projectNature"
                                    value="rtt.ui.projectnature">
                              </test>
                           </not>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
         <menu
               label="%menu.label">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IFile">
                     </adapt>
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.extension"
                           value="genmodel">
                     </test>
                  </iterate>
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
            <command
                  commandId="rtt.ui.commands.ecore.generateAnnotations"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:rtt.ui.views.LogView">
         <command
               commandId="rtt.ui.commands.comment.add"
               id="comment.add"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.testresult">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.comment.edit"
               id="comment.edit"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.comment">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.comment.remove"
               id="comment.remove"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.comment">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:rtt.ui.views.ProjectView">
         <command
               commandId="rtt.ui.commands.archive.check"
               icon="icons/eclipse/search.gif"
               id="archive.check"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.archive">
               </reference>
            </visibleWhen>
         </command>
         <separator
               name="rtt.ui.testsuiteview.seperator0"
               visible="true">
         </separator>
         <command
               commandId="rtt.ui.commands.config.add"
               icon="icons/eclipse/add.gif"
               id="config.add"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="rtt.ui.definitions.selection.directory.config">
                  </reference>
                  <reference
                        definitionId="rtt.ui.definitions.selection.config">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.config.edit"
               icon="icons/eclipse/properties.gif"
               id="config.edit"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="rtt.ui.definitions.selection.config">
                  </reference>
                  <reference
                        definitionId="rtt.ui.definitions.configParent">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.config.setActive"
               icon="icons/eclipse/synced.gif"
               id="config.setActive"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="rtt.ui.definitions.selection.config">
                  </reference>
                  <reference
                        definitionId="rtt.ui.definitions.configParent">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.testsuite.add"
               icon="icons/eclipse/add.gif"
               id="testsuite.add"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="rtt.ui.definitions.selection.directory.testsuite">
                  </reference>
                  <reference
                        definitionId="rtt.ui.definitions.selection.testsuite">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.testsuite.remove"
               icon="icons/eclipse/trash.gif"
               id="testsuite.remove"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.testsuite">
               </reference>
            </visibleWhen>
         </command>
         <separator
               name="rtt.ui.testsuiteview.seperator2"
               visible="true">
         </separator>
         <command
               commandId="rtt.ui.commands.testcase.add"
               icon="icons/eclipse/add.gif"
               id="testcase.add"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.testsuite">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.testcase.remove"
               icon="icons/eclipse/trash.gif"
               id="testcase.remove"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="rtt.ui.definitions.selection.testcase">
                  </reference>
                  <reference
                        definitionId="rtt.ui.definitions.caseParent">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.testcase.parameters"
               icon="icons/eclipse/settings.gif"
               id="testcase.parameters"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.testcase">
               </reference>
            </visibleWhen>
         </command>
         <separator
               name="rtt.ui.testsuiteview.seperator3"
               visible="true">
         </separator>
         <command
               commandId="rtt.ui.commands.tests.generate"
               icon="icons/eclipse/update.gif"
               id="tests.generate"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.testsuite">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="rtt.ui.commands.tests.run"
               icon="icons/eclipse/run.gif"
               id="tests.run"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="rtt.ui.definitions.selection.testsuite">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:rtt.ui.views.ProjectView">
         <command
               commandId="rtt.ui.commands.refresh"
               icon="icons/eclipse/refresh.gif"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
