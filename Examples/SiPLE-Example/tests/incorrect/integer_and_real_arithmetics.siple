Procedure main() Begin
	Var b:Boolean;
	Var i:Integer;
	Var r:Real;
	
	i := 3;
	r := 3.5;
	b := true;
	
	Write b + i;
	Write b + r;
	Write b - i;
	Write b - r;
	Write b * i;
	Write b * r;
	Write b / i;
	Write b / r;
	
	Write i + b;
	Write r + b;
	Write i - b;
	Write r - b;
	Write i * b;
	Write r * b;
	Write i / b;
	Write r / b;
	
	Write i * ((r + 2) - b) + r;
End;
